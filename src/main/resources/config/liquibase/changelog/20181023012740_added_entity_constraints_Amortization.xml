<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Amortization.
    -->
    <changeSet id="20181023012740-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="document_type_sell_id"
                                 baseTableName="amortization"
                                 constraintName="fk_amortization_document_type_sell_id"
                                 referencedColumnNames="id"
                                 referencedTableName="document_type_sell"/>

        <addForeignKeyConstraint baseColumnNames="payment_type_id"
                                 baseTableName="amortization"
                                 constraintName="fk_amortization_payment_type_id"
                                 referencedColumnNames="id"
                                 referencedTableName="payment_type"/>

        <addForeignKeyConstraint baseColumnNames="sell_id"
                                 baseTableName="amortization"
                                 constraintName="fk_amortization_sell_id"
                                 referencedColumnNames="id"
                                 referencedTableName="sell"/>

    </changeSet>
</databaseChangeLog>
