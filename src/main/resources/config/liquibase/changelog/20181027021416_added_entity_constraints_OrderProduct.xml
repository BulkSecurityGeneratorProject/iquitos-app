<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity OrderProduct.
    -->
    <changeSet id="20181027021416-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="provider_id"
                                 baseTableName="order_product"
                                 constraintName="fk_order_product_provider_id"
                                 referencedColumnNames="id"
                                 referencedTableName="provider"/>

        <addForeignKeyConstraint baseColumnNames="order_products_id"
                                 baseTableName="order_product_products"
                                 constraintName="fk_order_product_products_order_products_id"
                                 referencedColumnNames="id"
                                 referencedTableName="order_product"/>
        <addForeignKeyConstraint baseColumnNames="products_id"
                                 baseTableName="order_product_products"
                                 constraintName="fk_order_product_products_products_id"
                                 referencedColumnNames="id"
                                 referencedTableName="product"/>
        
    </changeSet>
</databaseChangeLog>
