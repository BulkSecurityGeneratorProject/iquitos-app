<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Purchase.
    -->
    <changeSet id="20181023012752-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="provider_id"
                                 baseTableName="purchase"
                                 constraintName="fk_purchase_provider_id"
                                 referencedColumnNames="id"
                                 referencedTableName="provider"/>

        <addForeignKeyConstraint baseColumnNames="document_type_purchase_id"
                                 baseTableName="purchase"
                                 constraintName="fk_purchase_document_type_purchase_id"
                                 referencedColumnNames="id"
                                 referencedTableName="document_type_purchase"/>

        <addForeignKeyConstraint baseColumnNames="purchase_status_id"
                                 baseTableName="purchase"
                                 constraintName="fk_purchase_purchase_status_id"
                                 referencedColumnNames="id"
                                 referencedTableName="purchase_status"/>

        <addForeignKeyConstraint baseColumnNames="box_id"
                                 baseTableName="purchase"
                                 constraintName="fk_purchase_box_id"
                                 referencedColumnNames="id"
                                 referencedTableName="box"/>

        <addForeignKeyConstraint baseColumnNames="purchases_id"
                                 baseTableName="purchase_products"
                                 constraintName="fk_purchase_products_purchases_id"
                                 referencedColumnNames="id"
                                 referencedTableName="purchase"/>
        <addForeignKeyConstraint baseColumnNames="products_id"
                                 baseTableName="purchase_products"
                                 constraintName="fk_purchase_products_products_id"
                                 referencedColumnNames="id"
                                 referencedTableName="product"/>
        
    </changeSet>
</databaseChangeLog>
